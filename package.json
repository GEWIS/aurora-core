{
  "name": "abc-narrowcasting",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "dev": "concurrently \"nodemon\" \"nodemon -x npx tsoa spec-and-routes\"",
    "build": "tsoa spec-and-routes && tsc",
    "start": "node dist/src/index.js",
    "tsoa": "tsoa spec-and-routes",
    "gen-client": "npm run gen-client-backoffice && npm run gen-client-client",
    "gen-client-backoffice": "openapi-ts --input ./build/swagger.json --output ../narrowcasting-backoffice/src/api/",
    "gen-client-client": "openapi-ts --input ./build/swagger.json --output ../narrowcasting-client/src/api/",
    "lint": "eslint ./src --ext .js,.jsx,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --max-warnings=0",
    "lint-fix": "eslint ./src --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --fix",
    "format": "prettier --ignore-path .gitignore --check ./src/",
    "format-fix": "prettier --ignore-path .gitignore --write ./src",
    "seed": "ts-node src/seed/index.ts",
    "prepare-husky": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fostertheweb/spotify-web-sdk": "^1.2.3",
    "@sudosos/sudosos-client": "github:GEWIS/sudosos-client",
    "@tsoa/runtime": "^5.0.0",
    "async-lock": "^1.4.1",
    "axios": "^1.6.8",
    "body-parser": "^1.20.2",
    "connect-typeorm": "^2.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "jwt-decode": "^4.0.0",
    "ldapts": "^7.0.11",
    "mysql2": "^3.9.7",
    "node-cron": "^3.0.3",
    "openid-client": "^5.6.5",
    "passport": "^0.6.0",
    "passport-custom": "^1.1.1",
    "pino": "^8.21.0",
    "pino-http": "^9.0.0",
    "pino-pretty": "^10.3.1",
    "qs": "^6.12.1",
    "socket.io": "^4.7.5",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^5.1.1",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.42.1",
    "@types/async-lock": "^1.4.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/node-cron": "^3.0.11",
    "@types/passport": "^1.0.16",
    "@types/passport-strategy": "^0.2.38",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "nswag": "^13.20.0",
    "openapi-typescript-codegen": "0.25.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
