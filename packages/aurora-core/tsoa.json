{
  "entryFile": "src/index.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": [
    "src/**/*-controller.ts"
  ],
  "spec": {
    "basePath": "/api",
    "securityDefinitions": {
      "local": {
        "type": "http",
        "name": "connect.sid",
        "in": "cookie"
      }
    },
    "outputDirectory": "build",
    "host": "",
    "specVersion": 3,
    "schemes": [
      "http",
      "https"
    ],
    "specMerging": "recursive",
    "spec": {
      "components": {
        "schemas": {
          "ApiKeyParameters": {
            "type": "object",
            "required": ["key"],
            "properties": {
              "key": {
                "type": "string"
              }
            },
            "additionalProperties": false
          }
        }
      },
      "paths": {
        "/auth/key": {
          "post": {
            "operationId": "AuthKey",
            "responses": {
              "200": {
                "description": "Success",
                "content": {
                  "application/json": {
                    "schema": {
                      "$ref": "#/components/schemas/AuthUser"
                    }
                  }
                }
              }
            },
            "security": [],
            "parameters": [],
            "tags": [
              "Authentication"
            ],
            "requestBody": {
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/ApiKeyParameters"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "routes": {
    "authenticationModule": "src/modules/auth/authentication.ts",
    "basePath": "/api",
    "routesDir": "build",
    "middleware": "express"
  }
}
